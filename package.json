{
  "name": "InfiniTex",
  "productName": "InfiniTex",
  "version": "0.4.7",
  "description": "A new age editor for academics who seek freedom and people with cryptography needs in rich text environments.",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "prebuild": "node ./beforeBundle.js",
    "build": "NODE_ENV=production BABEL_ENV=production browserify ./src/entryDev.js -t [ babelify --presets [ env --targets [ electron ] react ] --plugins transform-async-to-generator ] --full-path=false --exclude electron --exclude shelljs -o ./src/entry/bundle.js",
    "postbuild": "NODE_ENV=production uglifyjs --compress drop_console=true --mangle -- ./src/entry/bundle.js > ./src/react/inf.min.js",
    "package": "node ./beforePackage.js && cd dist && yarn && electron-forge package",
    "make": "node ./beforePackage.js && cd dist && yarn && electron-forge make && node ./afterMake.js"
  },
  "keywords": [],
  "author": "fetacore",
  "config": {
    "forge": "./forge.config.js"
  },
  "dependencies": {
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.2.3",
    "katex": "^0.9.0-beta1",
    "shelljs": "^0.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "brace": "^0.11.0",
    "browserify": "^15.2.0",
    "electron-forge": "^4.2.1",
    "electron-installer-debian": "^0.8.0",
    "electron-prebuilt-compile": "1.8.2-beta.4",
    "electron-reload": "^1.2.2",
    "jssoup": "0.0.6",
    "material-ui": "^0.20.0",
    "node-minify": "^2.4.1",
    "pdfjs-dist": "^2.0.303",
    "prop-types": "^15.6.0",
    "quill": "^1.3.5",
    "react": "^16.2.0",
    "react-ace": "^5.9.0",
    "react-color": "^2.13.8",
    "react-dom": "^16.2.0",
    "react-grid-layout": "^0.16.2",
    "react-hot-loader": "^3.1.3",
    "react-pdf": "^2.5.2",
    "react-quill": "^1.2.3",
    "react-resizable": "^1.7.5",
    "react-transition-group": "^2.2.1",
    "request": "^2.83.0",
    "uglify-js": "^3.3.9"
  }
}
